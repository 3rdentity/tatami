<?xml version="1.0" encoding="UTF-8"?>
<beans:beans xmlns="http://www.springframework.org/schema/security"
             xmlns:beans="http://www.springframework.org/schema/beans"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
				http://www.springframework.org/schema/security
				http://www.springframework.org/schema/security/spring-security-3.1.xsd"
        profile="default">

    <authentication-manager>
        <authentication-provider user-service-ref="userDetailsService"/>
    </authentication-manager>

    <user-service id="userDetailsService">
        <user name="user1" password="password" authorities="ROLE_USER"/>
        <user name="user2" password="password" authorities="ROLE_USER"/>
        <user name="user3" password="password" authorities="ROLE_USER"/>
        <user name="user4" password="password" authorities="ROLE_USER"/>
        <user name="user5" password="password" authorities="ROLE_USER"/>
        <user name="user6" password="password" authorities="ROLE_USER"/>
        <user name="user7" password="password" authorities="ROLE_USER"/>
        <user name="user8" password="password" authorities="ROLE_USER"/>
        <user name="user9" password="password" authorities="ROLE_USER"/>
        <user name="user10" password="password" authorities="ROLE_USER"/>
        <user name="user11" password="password" authorities="ROLE_USER"/>
        <user name="user12" password="password" authorities="ROLE_USER"/>
        <user name="user13" password="password" authorities="ROLE_USER"/>
        <user name="user14" password="password" authorities="ROLE_USER"/>
        <user name="user15" password="password" authorities="ROLE_USER"/>
    </user-service>

</beans:beans>